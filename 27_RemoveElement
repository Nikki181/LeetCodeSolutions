/* Given an array nums and a value val, remove all instances of that value in-place and return the new length. */

public class Solution {
 public int RemoveElement(int[] nums, int val) {
        int counter=0;
        for(int i=0; i< nums.Length; i++){
            if(nums[i] == val){
                continue;
            }
            nums[counter++] = nums[i];
        }
        return counter;
    }
}

